{
	"name": "frontend",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"dev-server": "vite dev --mode server",
		"dev-prod": "vite dev --mode prod",
		"build": "vite build",
		"build:node": "ADAPTER=node vite build",
		"build:vercel": "ADAPTER=vercel vite build",
		"build:static": "ADAPTER=static vite build",
		"preview": "vite preview",
		"run:node-env": "node -r dotenv/config build-node",
		"run:node-sv08ludweiler.de": "node -r dotenv/config build-node dotenv_config_path=.env.server.local",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier . --check . && eslint .",
		"format": "prettier --write .",
		"prepare": "npm run smui-theme-light && npm run smui-theme-dark",
		"smui-theme-light": "smui-theme compile static/smui.css -i src/theme",
		"smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark",
		"sveltekit:sync": "svelte-kit sync",
		"pwa:generate:assets": "pwa-assets-generator --preset minimal ./src/lib/assets/wappen.png -m"
	},
	"devDependencies": {
		"@smui/button": "7.0.0",
		"@smui/card": "7.0.0",
		"@sveltejs/adapter-auto": "3.2.4",
		"@sveltejs/adapter-node": "5.2.2",
		"@sveltejs/adapter-static": "3.0.4",
		"@sveltejs/adapter-vercel": "5.4.3",
		"@sveltejs/kit": "2.5.25",
		"@typescript-eslint/eslint-plugin": "8.3.0",
		"@typescript-eslint/parser": "8.3.0",
		"autoprefixer": "10.4.20",
		"dotenv": "16.4.5",
		"eslint": "9.9.1",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-svelte": "2.43.0",
		"postcss": "8.4.41",
		"qs": "6.13.0",
		"smui-theme": "7.0.0",
		"svelte": "4.2.19",
		"svelte-check": "3.8.6",
		"tailwindcss": "3.4.10",
		"tslib": "2.7.0",
		"typescript": "5.5.4",
		"vercel": "37.2.1",
		"vite": "5.4.2",
		"vitest": "2.0.5",
		"@rollup/plugin-node-resolve": "15.2.3",
		"@rollup/plugin-commonjs": "26.0.1",
		"@rollup/plugin-json": "6.1.0",
		"vite-plugin-pwa": "0.20.2",
		"@vite-pwa/sveltekit": "0.6.1",
		"@vite-pwa/assets-generator": "0.2.4",
		"@sveltejs/enhanced-img": "0.3.4"
	},
	"dependencies": {
		"mdsvex": "0.12.3"
	},
	"type": "module"
}
