{
	"name": "frontend",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"dev-server": "vite dev --mode server",
		"build": "vite build",
		"build:node": "ADAPTER=node vite build",
		"build:static": "ADAPTER=static vite build",
		"preview": "vite preview",
		"run:node-env": "node -r dotenv/config build-node",
		"run:node-sv08ludweiler.de": "node -r dotenv/config build-node dotenv_config_path=.env.server.local",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier . --check . && eslint .",
		"format": "prettier --write .",
		"prepare": "npm run smui-theme-light && npm run smui-theme-dark",
		"smui-theme-light": "smui-theme compile static/smui.css -i src/theme",
		"smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark",
		"sveltekit:sync2": "svelte-kit sync"
	},
	"devDependencies": {
		"@smui/button": "7.0.0-beta.14",
		"@smui/card": "7.0.0-beta.14",
		"@sveltejs/adapter-auto": "2.1.0",
		"@sveltejs/adapter-node": "1.3.1",
		"@sveltejs/adapter-static": "2.0.3",
		"@sveltejs/kit": "1.22.6",
		"@typescript-eslint/eslint-plugin": "6.3.0",
		"@typescript-eslint/parser": "6.3.0",
		"autoprefixer": "10.4.15",
		"dotenv": "16.3.1",
		"eslint": "8.47.0",
		"eslint-config-prettier": "9.0.0",
		"eslint-plugin-svelte": "2.32.4",
		"postcss": "8.4.27",
		"qs": "6.11.2",
		"smui-theme": "7.0.0-beta.14",
		"svelte": "4.2.0",
		"svelte-check": "3.5.0",
		"tailwindcss": "3.3.3",
		"tslib": "2.6.1",
		"typescript": "5.1.6",
		"vite": "4.4.9",
		"vitest": "0.34.1"
	},
	"dependencies": {
		"mdsvex": "0.11.0"
	},
	"type": "module"
}