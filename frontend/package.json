{
	"name": "frontend",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev --host",
		"dev-server": "vite dev --mode server --host",
		"dev-prod": "vite dev --mode prod --host",
		"build": "vite build",
		"build:node": "ADAPTER=node vite build",
		"build:vercel": "ADAPTER=vercel vite build",
		"build:static": "ADAPTER=static vite build",
		"preview": "vite preview",
		"run:node-env": "node -r dotenv/config build-node",
		"run:node-sv08ludweiler.de": "node -r dotenv/config build-node dotenv_config_path=.env.server.local",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier . --check . && eslint .",
		"format": "prettier --write .",
		"prepare": "npm run smui-theme-light && npm run smui-theme-dark",
		"smui-theme-light": "smui-theme compile static/smui.css -i src/theme",
		"smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark",
		"sveltekit:sync": "svelte-kit sync",
		"pwa:generate:assets": "pwa-assets-generator --preset minimal ./src/lib/assets/wappen.png -m"
	},
	"devDependencies": {
		"@smui/button": "8.0.0-beta.3",
		"@smui/card": "8.0.0-beta.3",
		"@sveltejs/adapter-auto": "3.3.1",
		"@sveltejs/adapter-node": "5.2.11",
		"@sveltejs/adapter-static": "3.0.8",
		"@sveltejs/adapter-vercel": "5.5.2",
		"@sveltejs/kit": "2.15.1",
		"@sveltejs/vite-plugin-svelte": "5.0.3",
		"@typescript-eslint/eslint-plugin": "8.19.0",
		"@typescript-eslint/parser": "8.19.0",
		"autoprefixer": "10.4.20",
		"dotenv": "16.4.7",
		"eslint": "9.17.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-svelte": "2.46.1",
		"postcss": "8.4.49",
		"qs": "6.13.1",
		"smui-theme": "8.0.0-beta.3",
		"svelte": "5.16.1",
		"svelte-check": "4.1.1",
		"tailwindcss": "3.4.17",
		"tslib": "2.8.1",
		"typescript": "5.7.2",
		"vercel": "39.2.5",
		"vite": "6.0.7",
		"vitest": "2.1.6",
		"@rollup/plugin-node-resolve": "16.0.0",
		"@rollup/plugin-commonjs": "28.0.2",
		"@rollup/plugin-json": "6.1.0",
		"vite-plugin-pwa": "0.21.1",
		"@vite-pwa/sveltekit": "0.6.7",
		"@vite-pwa/assets-generator": "0.2.6",
		"@sveltejs/enhanced-img": "0.4.4"
	},
	"dependencies": {
		"mdsvex": "0.12.3"
	},
	"type": "module"
}
